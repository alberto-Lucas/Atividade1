<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Atividade1.PgMenuShell"
       xmlns:local="clr-namespace:Atividade1"
       FlyoutBackgroundColor="{StaticResource Menu}"
       Shell.BackgroundColor="Yellow"
       Shell.TitleColor="Green"
       Shell.ForegroundColor="Green"
       >

    <FlyoutItem Route="principal"
                FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Principal"
                      ContentTemplate=
                      "{DataTemplate local:Principal}"
                      Icon="home.png"/>
    </FlyoutItem>
    

</Shell>